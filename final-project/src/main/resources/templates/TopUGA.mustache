        <td>{{first_name}}</td>
        <td>{{last_name}}</td>
        <td>{{total_receiving_yards}}</td>
        <!-- more columns as needed -->
    </tr>
    {{/te}}
    </table>
    <hr/>
    <h2>Linebacker</h2>
    <table>
    <tr>
        <!-- Table headers -->
        <th>First Name</th>
