        <td>{{total_punt_yards}}</td>
        <!-- more columns as needed -->
    </tr>
    {{/p}}
    </table>
    <hr/>
    <h2>Kicker</h2>
    <table>
    <tr>
        <!-- Table headers -->
        <th>First Name</th>
        <th>Last Name</th>
        <th>Points</th>
        <!-- more columns as needed -->
    </tr>
    {{#pk}}
